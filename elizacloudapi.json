{
    "openapi": "3.0.3",
    "info": {
      "title": "Eliza Cloud API",
      "description": "AI agent development platform with multi-model text generation, image creation, and enterprise features",
      "version": "1.0.0",
      "contact": {
        "name": "Eliza Cloud",
        "url": "https://github.com/elizaOS/eliza-cloud-v2"
      }
    },
    "servers": [
      {
        "url": "http://localhost:3000",
        "description": "Production server"
      }
    ],
    "security": [
      {
        "bearerAuth": []
      },
      {
        "apiKeyAuth": []
      }
    ],
    "components": {
      "securitySchemes": {
        "bearerAuth": {
          "type": "http",
          "scheme": "bearer",
          "bearerFormat": "JWT",
          "description": "Privy session authentication"
        },
        "apiKeyAuth": {
          "type": "apiKey",
          "in": "header",
          "name": "Authorization",
          "description": "API Key authentication (Bearer <key>)"
        }
      }
    },
    "paths": {
      "/api/v1/generate-image": {
        "post": {
          "operationId": "generate-image",
          "summary": "Generate Image",
          "description": "Generate images from text prompts using AI models (supports API key auth)",
          "tags": [
            "Image Generation"
          ],
          "responses": {
            "200": {
              "description": "Image generated successfully"
            },
            "400": {
              "description": "Invalid request parameters"
            },
            "401": {
              "description": "Authentication required"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ],
          "requestBody": {
            "description": "Request body",
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "prompt": {
                      "type": "string",
                      "description": "Text description of the desired image",
                      "example": "A futuristic city with flying cars and neon lights",
                      "default": "A beautiful mountain landscape at sunset"
                    }
                  },
                  "required": [
                    "prompt"
                  ]
                }
              }
            }
          }
        }
      },
      "/api/v1/generate-video": {
        "post": {
          "operationId": "generate-video",
          "summary": "Generate Video",
          "description": "Generate videos from text prompts (supports API key auth)",
          "tags": [
            "Video Generation"
          ],
          "responses": {
            "200": {
              "description": "Video generated successfully"
            },
            "400": {
              "description": "Invalid request parameters"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ],
          "requestBody": {
            "description": "Request body",
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "prompt": {
                      "type": "string",
                      "description": "Text description of the desired video",
                      "example": "A futuristic city with flying cars at night",
                      "default": "A serene mountain landscape with clouds moving slowly"
                    }
                  },
                  "required": [
                    "prompt"
                  ]
                }
              }
            }
          }
        }
      },
      "/api/v1/chat": {
        "post": {
          "operationId": "chat-completions",
          "summary": "Chat Completion",
          "description": "Generate text completions using Vercel AI SDK format (supports API key auth)",
          "tags": [
            "AI Completions"
          ],
          "responses": {
            "200": {
              "description": "Text generated successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ],
          "requestBody": {
            "description": "Request body",
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "messages": {
                      "type": "array",
                      "description": "Array of UIMessage objects (Vercel AI SDK format with role and parts)",
                      "example": "[{\"role\":\"user\",\"parts\":[{\"type\":\"text\",\"text\":\"Explain quantum computing\"}]}]",
                      "default": "[{\"role\":\"user\",\"parts\":[{\"type\":\"text\",\"text\":\"Hello, how are you?\"}]}]",
                      "items": {
                        "type": "string"
                      }
                    },
                    "id": {
                      "type": "string",
                      "description": "Model to use for completion",
                      "example": "gpt-4o-mini",
                      "default": "gpt-4o"
                    }
                  },
                  "required": [
                    "messages"
                  ]
                }
              }
            }
          }
        }
      },
      "/api/v1/character-assistant": {
        "post": {
          "operationId": "character-assistant",
          "summary": "Character Assistant",
          "description": "AI assistant for creating character definitions (session auth only)",
          "tags": [
            "AI Completions"
          ],
          "responses": {
            "200": {
              "description": "Character assistant response generated"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ],
          "requestBody": {
            "description": "Request body",
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "messages": {
                      "type": "array",
                      "description": "Conversation messages (Vercel AI SDK UIMessage format)",
                      "example": "[{\"role\":\"user\",\"parts\":[{\"type\":\"text\",\"text\":\"Create a sci-fi character\"}]}]",
                      "default": "[{\"role\":\"user\",\"parts\":[{\"type\":\"text\",\"text\":\"Help me create a character\"}]}]",
                      "items": {
                        "type": "string"
                      }
                    },
                    "characterData": {
                      "type": "object",
                      "description": "Existing character data to refine",
                      "example": "{\"name\": \"Agent X\"}",
                      "default": "{}"
                    }
                  },
                  "required": [
                    "messages"
                  ]
                }
              }
            }
          }
        }
      },
      "/api/v1/generate-prompts": {
        "post": {
          "operationId": "generate-prompts",
          "summary": "Generate Prompts",
          "description": "Generate creative prompts for image/video generation (session auth only)",
          "tags": [
            "AI Completions"
          ],
          "responses": {
            "200": {
              "description": "Prompts generated successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ],
          "requestBody": {
            "description": "Request body",
            "required": false,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "seed": {
                      "type": "number",
                      "description": "Seed for prompt generation (optional, auto-generated if not provided)",
                      "example": 1234567890
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/v1/models": {
        "get": {
          "operationId": "models-list",
          "summary": "List Models",
          "description": "List all available AI models (supports API key auth)",
          "tags": [
            "Models"
          ],
          "responses": {
            "200": {
              "description": "Models retrieved successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ]
        }
      },
      "/api/v1/gallery": {
        "get": {
          "operationId": "gallery-list",
          "summary": "List Generations",
          "description": "List all media generations (images and videos) (supports API key auth)",
          "tags": [
            "Gallery"
          ],
          "responses": {
            "200": {
              "description": "Generations retrieved successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ],
          "parameters": [
            {
              "name": "type",
              "in": "query",
              "required": false,
              "description": "Filter by media type",
              "schema": {
                "type": "string",
                "description": "Filter by media type",
                "enum": [
                  "image",
                  "video"
                ],
                "example": "image"
              }
            },
            {
              "name": "limit",
              "in": "query",
              "required": false,
              "description": "Maximum number of results",
              "schema": {
                "type": "number",
                "description": "Maximum number of results",
                "example": 50,
                "default": 100
              }
            },
            {
              "name": "offset",
              "in": "query",
              "required": false,
              "description": "Pagination offset",
              "schema": {
                "type": "number",
                "description": "Pagination offset",
                "example": 0,
                "default": 0
              }
            }
          ]
        }
      },
      "/api/v1/user": {
        "get": {
          "operationId": "user-get",
          "summary": "Get User Profile",
          "description": "Get current user profile and organization details (session auth only - won't work with API key)",
          "tags": [
            "User Management"
          ],
          "responses": {
            "200": {
              "description": "User profile retrieved successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ]
        },
        "patch": {
          "operationId": "user-update",
          "summary": "Update User Profile",
          "description": "Update user profile information (session auth only - won't work with API key)",
          "tags": [
            "User Management"
          ],
          "responses": {
            "200": {
              "description": "Profile updated successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ],
          "requestBody": {
            "description": "Request body",
            "required": false,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "User's display name",
                      "example": "John Doe"
                    },
                    "avatar": {
                      "type": "string",
                      "description": "Avatar URL",
                      "example": "https://example.com/avatar.jpg"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/v1/api-keys": {
        "get": {
          "operationId": "api-keys-list",
          "summary": "List API Keys",
          "description": "List all API keys for your organization (session auth only - won't work with API key)",
          "tags": [
            "API Keys"
          ],
          "responses": {
            "200": {
              "description": "API keys retrieved successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ]
        },
        "post": {
          "operationId": "api-keys-create",
          "summary": "Create API Key",
          "description": "Create a new API key (session auth only - won't work with API key)",
          "tags": [
            "API Keys"
          ],
          "responses": {
            "201": {
              "description": "API key created successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ],
          "requestBody": {
            "description": "Request body",
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "Name for the API key",
                      "example": "Production API Key",
                      "default": "Test Key"
                    },
                    "description": {
                      "type": "string",
                      "description": "Optional description",
                      "example": "Used for production services"
                    },
                    "permissions": {
                      "type": "array",
                      "description": "Array of permissions",
                      "example": "[\"read\", \"write\"]",
                      "default": "[]",
                      "items": {
                        "type": "string"
                      }
                    },
                    "rate_limit": {
                      "type": "number",
                      "description": "Rate limit per minute",
                      "example": 1000,
                      "default": 1000
                    }
                  },
                  "required": [
                    "name"
                  ]
                }
              }
            }
          }
        }
      },
      "/api/v1/api-keys/{id}": {
        "delete": {
          "operationId": "api-keys-delete",
          "summary": "Delete API Key",
          "description": "Delete an API key (session auth only - won't work with API key)",
          "tags": [
            "API Keys"
          ],
          "responses": {
            "200": {
              "description": "API key deleted successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ],
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "description": "API key ID",
              "schema": {
                "type": "string",
                "description": "API key ID",
                "example": "key_123abc"
              }
            }
          ]
        },
        "patch": {
          "operationId": "api-keys-update",
          "summary": "Update API Key",
          "description": "Update API key properties (session auth only - won't work with API key)",
          "tags": [
            "API Keys"
          ],
          "responses": {
            "200": {
              "description": "API key updated successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
            {
              "apiKeyAuth": []
            }
          ],
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "description": "API key ID",
              "schema": {
                "type": "string",
                "description": "API key ID",
                "example": "key_123abc"
              }
            }
          ],
          "requestBody": {
            "description": "Request body",
            "required": false,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "New name for the API key",
                      "example": "Updated Key Name"
                    },
                    "is_active": {
                      "type": "boolean",
                      "description": "Enable or disable the key",
                      "example": true
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/api/v1/api-keys/{id}/regenerate": {
        "post": {
          "operationId": "api-keys-regenerate",
          "summary": "Regenerate API Key",
          "description": "Regenerate API key secret (old key becomes invalid) (session auth only - won't work with API key)",
          "tags": [
            "API Keys"
          ],
          "responses": {
            "200": {
              "description": "API key regenerated successfully"
            }
          },
          "security": [
            {
              "bearerAuth": []
            },
